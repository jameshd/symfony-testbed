<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}

        {% block debug %}
            <ul>
                <li>Env: {{app.environment}}</li>
                <li>User: {{ app.user}} </li>
                <li>Request: {{ app.request}}</li>
                <li>Session: {{app.session.isStarted}}</li>
                <li>Debug: {{app.debug}}</li>
            </ul>

        {% endblock  %}

        {% block javascripts %}{% endblock %}
    </body>
</html>
